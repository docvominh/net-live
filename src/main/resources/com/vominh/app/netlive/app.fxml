<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.vominh.app.netlive.controller.AppController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <TabPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="700.0" tabClosingPolicy="UNAVAILABLE">
     <tabs>
       <Tab text="System information">
         <content>
           <AnchorPane minHeight="-Infinity" minWidth="-Infinity">
                  <children>
                     <GridPane layoutX="14.0" layoutY="13.0">
                       <columnConstraints>
                         <ColumnConstraints hgrow="SOMETIMES" maxWidth="335.0" minWidth="10.0" prefWidth="199.99998474121094" />
                         <ColumnConstraints hgrow="SOMETIMES" maxWidth="507.00001525878906" minWidth="10.0" prefWidth="482.00001525878906" />
                       </columnConstraints>
                       <rowConstraints>
                         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                           <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                           <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                           <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                           <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                       </rowConstraints>
                        <children>
                           <Label styleClass="bold" text="Operation System" />
                           <Label fx:id="os" GridPane.columnIndex="1" />
                           <Label text="Chip" GridPane.rowIndex="1" />
                           <Label fx:id="chip" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                           <Label text="Memory" GridPane.rowIndex="2" />
                           <Label fx:id="memory" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                        </children>
                     </GridPane>
                  </children>
               </AnchorPane>
         </content>
       </Tab>
       <Tab text="Live Ping">
         <content>
           <AnchorPane minHeight="-Infinity" minWidth="-Infinity">
                  <children>
                     <GridPane layoutX="14.0" layoutY="14.0" prefHeight="30.0" prefWidth="500.0">
                       <columnConstraints>
                         <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                         <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                           <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                       </columnConstraints>
                       <rowConstraints>
                         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                       </rowConstraints>
                        <children>
                           <TextField>
                              <GridPane.margin>
                                 <Insets left="10.0" right="10.0" />
                              </GridPane.margin></TextField>
                           <TextField GridPane.columnIndex="1">
                              <GridPane.margin>
                                 <Insets right="10.0" />
                              </GridPane.margin>
                           </TextField>
                           <Button mnemonicParsing="false" text="Add Server" GridPane.columnIndex="2" />
                        </children>
                     </GridPane>
                  </children></AnchorPane>
         </content>
       </Tab>
     </tabs>
   </TabPane>
</VBox>
